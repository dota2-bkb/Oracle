# Streamlit secrets configuration
# RENAME this file to "secrets.toml" and place it in ".streamlit/" folder locally
# or paste content into Streamlit Cloud Secrets management.

[opendota]
api_key = "YOUR_API_KEY_HERE"

[database]
# For production, replace with Postgres URL
url = "sqlite:///./dota2_analyst.db"

[auth]
# Simple password for internal access
password = "team_secret_password"

